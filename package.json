    "babel-loader": "^8.4.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.6",
    "lint-staged": "^15.4.2",
    "msw-storybook-addon": "^2.0.4",
    "prettier": "^3.4.2",
    "react-test-renderer": "18.3.1",
    "reactotron-react-native": "^5.1.13",
    "typescript": "^5.3.3"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
