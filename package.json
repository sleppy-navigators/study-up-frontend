    "expo": "~52.0.46",
    "expo-blur": "~14.0.3",
    "expo-build-properties": "~0.13.3",
    "expo-clipboard": "^7.0.1",
    "expo-constants": "~17.0.8",
    "expo-dev-client": "~5.0.19",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
