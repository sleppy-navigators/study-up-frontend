{
  "name": "stickersmash",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "packageManager": "bun@1.1.43",
  "scripts": {
    "dev": "eas build --profile development",
    "preview": "eas build --profile preview",
    "prod": "eas build --profile production",
    "start": "cross-env APP_VARIANT=development expo start --dev-client --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "cross-env APP_VARIANT=development expo run:android",
    "ios": "cross-env APP_VARIANT=development expo run:ios",
    "web": "cross-env APP_VARIANT=development expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "lint-fix": "eslint . --fix",
    "format": "prettier --write .",
    "test-deeplink": "adb shell am start -W -a android.intent.action.VIEW -d \"com.sleppynavigators.studyup://invitations/1?invitationKey=1dbf550e-8d74-4b6a-831b-879f0ca1b514\" com.sleppynavigators.studyup",
    "test-deeplink:dev": "adb shell am start -W -a android.intent.action.VIEW -d \"exp+studyup://invitations/1?invitationKey=1dbf550e-8d74-4b6a-831b-879f0ca1b514\" com.sleppynavigators.studyup",
    "storybook-generate": "sb-rn-get-stories",
    "start-storybook": "cross-env STORYBOOK_ENABLED='true' bun start",
    "start-storybook:ios": "cross-env STORYBOOK_ENABLED='true' bun ios",
    "start-storybook:android": "cross-env STORYBOOK_ENABLED='true' bun android",
    "act:build": "act --container-architecture linux/amd64 --platform ubuntu-latest=catthehacker/ubuntu:act-latest,macos-latest=-self-hosted --workflows .github/workflows/build.yml --input-file act/build/.inputs --secret-file act/build/.secrets --env-file act/build/.env"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@lukemorales/query-key-factory": "^1.3.4",
    "@react-native-community/datetimepicker": "8.2.0",
    "@react-native-firebase/app": "^21.7.1",
    "@react-native-firebase/auth": "^21.7.1",
    "@react-native-google-signin/google-signin": "^13.1.0",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@stomp/stompjs": "^7.0.0",
